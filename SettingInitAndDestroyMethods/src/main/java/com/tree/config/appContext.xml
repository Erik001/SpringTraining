<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="fruit" class="com.tree.test.Fruit" init-method="initMethod"
		scope="singleton" destroy-method="destroyMethod">
		<property name="description"
			value="Hi I am a fruit. I come from plants or trees with seeds." />
	</bean>
	<bean id="vegetable" class="com.tree.test.Vegetable" />
	<bean id="dairy" class="com.tree.test.Dairy" />
	<bean id="meat" class="com.tree.test.Meat" />
	<bean id="grain" class="com.tree.test.Grain" />

</beans>
